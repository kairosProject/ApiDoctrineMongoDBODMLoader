{
    "name": "kairos-project/api-doctrine-mongodb-odm-loader",
    "description": "The doctrine ODM component of the kairos API. Aim to be used as data access object for mongoDB",
    "type": "library",
    "require": {
        "kairos-project/api-loader": "^1@dev",
        "doctrine/mongodb-odm": "^2@dev"
    },
	"require-dev" : {
		"infection/infection" : "^0.8.2",
		"phpmd/phpmd" : "^2.6",
		"phpmetrics/phpmetrics" : "^2.3",
		"phpunit/phpunit" : "^7.2",
		"sebastian/phpcpd" : "^4",
		"squizlabs/php_codesniffer" : "^3.3",
		"kairos-project/tests": "^1@dev"
	},
    "license": "MIT",
    "authors": [
        {
            "name": "matthieu88160",
            "email": "matthieu.vallance@cscfa.fr"
        }
    ],
    "minimum-stability": "dev",
	"autoload" : {
		"psr-4" : {
			"KairosProject\\ApiDoctrineMongoDBODMLoader\\" : "KairosProject/ApiDoctrineMongoDBODMLoader/"
		}
	},
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
	}
}
